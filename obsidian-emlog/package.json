{
	"name": "obsidian-emlog",
	"version": "0.1.0",
	"description": "Publish/update Obsidian notes to EMLOG.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:integration": "RUN_EMLOG_TESTS=1 vitest run tests/emlog.integration.test.ts",
		"verify": "bash scripts/verify-emlog.sh"
	},
	"keywords": [
		"obsidian",
		"emlog",
		"publisher"
	],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4",
		"vitest": "^1.6.0"
	},
	"dependencies": {
		"blueimp-md5": "^2.19.0"
	}
}
